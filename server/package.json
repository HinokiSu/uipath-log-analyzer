{
  "name": "uipath-log-analyzer_server",
  "version": "1.0.0",
  "author": "Hinoki",
  "main": "index.js",
  "license": "MIT",
  "bin": "dist/index.js",
  "repository": {
    "url": "https://github.com/HinokiSu/uipath-log-analyzer"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "clean": "rimraf dist/ && rimraf src/public/",
    "copy-app-dist": "copyfiles -u 3 \"../app/dist/**/*.*\" \"./src/public/\"",
    "copy-views": "yarn copy-app-dist && copyfiles -u 2 \"./src/public/**/*.*\" \"./dist/public/\"",
    "build": "yarn clean && yarn tsc && yarn copy-views",
    "preview": "node dist/index.js",
    "watch:server": "nodemon  --watch src/**/* -e ts,tsx --exec ts-node src/index.ts",
    "dev:server": "set NODE_ENV=dev&& nodemon  --watch src/**/* -e ts,tsx --exec ts-node src/index.ts",
    "copy-server-config": "cp server.config.json ./build/",
    "prod:pkg": "yarn build && pkg . && yarn run copy-server-config"
  },
  "pkg": {
    "assets": [
      "dist/public/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "build"
  },
  "dependencies": {
    "@types/better-sqlite3": "^7.6.2",
    "@types/co": "^4.6.3",
    "@types/cors": "^2.8.13",
    "@types/uuid": "^9.0.0",
    "better-sqlite3": "^8.0.0",
    "co": "^4.6.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
